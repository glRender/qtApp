#version 330 core 
uniform mat4 model; 
uniform mat4 view; 
uniform mat4 projection; 
layout (location = 0) in vec3 vertex; 
out vec3 keyring_position; 
void main(void) 
{ 
   keyring_position = vertex; 
   gl_Position      = projection * view * model * vec4 ( vertex.x, vertex.y, vertex.z, 1.0 ); 
}